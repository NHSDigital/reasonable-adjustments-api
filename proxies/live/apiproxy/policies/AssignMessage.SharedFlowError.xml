<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage name="AssignMessage.SharedFlowError">
    <AssignVariable>
        <Name>ra.error.code</Name>
        <Template>{error.status.code}</Template>
    </AssignVariable>
    <AssignVariable>
        <Name>ra.error.coding.code</Name>
        <Template>{error.status.code}</Template>
    </AssignVariable>
    <AssignVariable>
        <Name>ra.error.coding.display</Name>
        <Template>{error.reason.phrase}</Template>
    </AssignVariable>
    <AssignVariable>
        <Name>ra.error.diagnostics</Name>
        <Template>{error.content}</Template>
    </AssignVariable>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</AssignMessage>
