<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AssignMessage.InvalidAccessToken">
    <AssignVariable>
        <Name>ra.error.code</Name>
        <Value>forbidden</Value>
    </AssignVariable>
    <AssignVariable>
        <Name>ra.error.coding.code</Name>
        <Value>ACCESS_DENIED</Value>
    </AssignVariable>
    <AssignVariable>
        <Name>ra.error.coding.display</Name>
        <Value>Access Denied - Unauthorised</Value>
    </AssignVariable>
    <AssignVariable>
        <Name>ra.error.diagnostics</Name>
        <Template>Access token is invalid or expired</Template>
    </AssignVariable>
    <AssignVariable>
        <Name>ra.error.status_code</Name>
        <Value>401</Value>
    </AssignVariable>
    <AssignVariable>
        <Name>ra.error.reason_phrase</Name>
        <Value>Unauthorized</Value>
    </AssignVariable>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</AssignMessage>
