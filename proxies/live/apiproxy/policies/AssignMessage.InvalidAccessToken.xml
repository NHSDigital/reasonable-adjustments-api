<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage name="AssignMessage.InvalidAccessToken">
    <AssignVariable>
        <Name>ra.error.code</Name>
        <Value>forbidden</Value>
    </AssignVariable>
    <AssignVariable>
        <Name>ra.error.coding.code</Name>
        <Value>ACCESS DENIED</Value>
    </AssignVariable>
    <AssignVariable>
        <Name>ra.error.coding.display</Name>
        <Value>Access token is invalid or expired</Value>
    </AssignVariable>
    <AssignVariable>
        <Name>ra.error.diagnostics</Name>
        <Template>Access token is invalid or expired</Template>
    </AssignVariable>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</AssignMessage>