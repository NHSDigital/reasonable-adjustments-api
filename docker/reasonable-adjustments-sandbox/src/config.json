{
    "mockDirectory": "./src/mocks",
    "quiet": false,
    "port": "9000",
    "latency": 50,
    "logRequestHeaders": false,
    "webServices": {
        "_status": {
            "latency": 20,
            "verbs": ["get"],
            "responses": {
                "get": {"httpStatus": 200, "mockFile": "status.json"}
            }
      },
      "consent": {
          "latency": 20,
          "verbs": ["get","post"],
          "responses": {
                "get": {"httpStatus": 200, "mockFile": "readConsent.json"},
                "post": {"httpStatus": 201, "mockFile": "createConsent.json"}
          }
      }
    }
}
